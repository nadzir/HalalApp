{
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "no-console": 0,
      "comma-dangle": 1,
      "indent": [2, 2],
      "no-plusplus": 0,
      "func-names": 0,
      "arrow-body-style": ["error", "as-needed"],
      "no-param-reassign": 0,
      "space-before-function-paren": 0,
      "react/forbid-prop-types": 0,
      "react/no-find-dom-node": 0,
      "react/no-multi-comp": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    },
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ]
  }
